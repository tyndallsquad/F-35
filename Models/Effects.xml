	<model>
	  <name>g-vortexR</name>
		<path>Aircraft/F-35/Models/Effects/Vapour/g-vortex.xml</path>
		<offsets>
			<x-m>1.0</x-m>
			<y-m>2.4</y-m>
			<z-m>0.2</z-m>
			<roll-deg>-5</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>

<!--left-->
	<model>
  <name>g-vortexL</name>
		<path>Aircraft/F-35/Models/Effects/Vapour/g-vortex.xml</path>
		<offsets>
			<x-m>1.0</x-m>
			<y-m>-2.4</y-m>
			<z-m>0.2</z-m>
			<roll-deg>5</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>
		<model>
  <name>left-strake-vortex2</name>
		<path>Aircraft/F-35/Models/Effects/Vapour/strake-vortex.xml</path>
		<offsets>
    			<x-m>-2.9</x-m>
			<y-m>-1.0</y-m>
			<z-m>0.4</z-m>
			<roll-deg>5</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>
		<model>
  <name>right-strake-vortex2</name>
		<path>Aircraft/F-35/Models/Effects/Vapour/strake-vortex.xml</path>
		<offsets>
    			<x-m>-2.9</x-m>
			<y-m>1.0</y-m>
			<z-m>0.4</z-m>
			<roll-deg>5</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>

	
		<model>
  <name>right-strake-vortex</name>
		<path>Aircraft/F-35/Models/Effects/Vapour/strake-vortex.xml</path>
		<offsets>
    			<x-m>-3.2</x-m>
			<y-m>1.458</y-m>
			<z-m>0.2</z-m>
			<roll-deg>5</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>
		<model>
  <name>left-strake-vortex</name>
		<path>Aircraft/F-35/Models/Effects/Vapour/strake-vortex.xml</path>
		<offsets>
    			<x-m>-3.2</x-m>
			<y-m>-1.458</y-m>
			<z-m>0.2</z-m>
			<roll-deg>5</roll-deg>
			<pitch-deg>0</pitch-deg>
			<heading-deg>0</heading-deg>
		</offsets>
	</model>
 <animation>
    <type>select</type>
    <object-name>right-strake-vortex</object-name>
    <object-name>left-strake-vortex</object-name>
    <object-name>right-strake-vortex2</object-name>
    <object-name>left-strake-vortex2</object-name>
    <condition>
       <and>

          <greater-than>
           <property>sim/multiplay/generic/float[31]</property>
           <value>7</value>
          </greater-than>
          <greater-than>
           <property>sim/multiplay/generic/float[1]</property>
           <value>130</value>
          </greater-than>
       </and>
    </condition>
  </animation>
 <animation>
    <type>select</type>
    <object-name>g-vortexL</object-name>
    <object-name>g-vortexR</object-name>

    <condition>
       <and>

          <greater-than>
           <property>sim/multiplay/generic/float[31]</property>
           <value>10</value>
          </greater-than>
          <greater-than>
           <property>sim/multiplay/generic/float[1]</property>
           <value>230</value>
          </greater-than>
       </and>
    </condition>
  </animation>
	



	<model>
		<name>sonicboom</name>
		<path>Aircraft/F-35/Models/Effects/Vapour/mach-cone.xml</path>
		<offsets>
			<x-m>1</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
	</model>
	
	<animation>
      <type>select</type>
      <object-name>sonicboom</object-name>
      <condition>
      <greater-than>
       <property>sim/multiplay/generic/float[2]</property><!-- velocities/mach -->
       <value>0.98</value>
      </greater-than>
      <less-than>
       <property>sim/multiplay/generic/float[2]</property><!-- velocities/mach -->
       <value>1.0</value>
      </less-than>
      </condition>
	  </animation>


