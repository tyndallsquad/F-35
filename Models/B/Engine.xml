<PropertyList>

	<path>HybridEngine.ac</path>
	
	
	<model>
    <name>plume</name>
    <path>Aircraft/F-35/Models/Effects/Afterburner/AfterBurner.xml</path>
	<offsets>
	<x-m>6.350</x-m>
         <y-m>0</y-m>
         <z-m>0.185</z-m>
         <pitch-deg>0</pitch-deg>
	</offsets>
  </model>



   <model>
    <name>Nozzle</name>
    <path>Aircraft/F-35/Models/Nozzle/nozzle.xml</path>
	<offsets>
	<x-m>4.835</x-m>
         <y-m>0.015</y-m>
         <z-m> 0.240</z-m>
     <pitch-deg>0</pitch-deg>
	</offsets>
   </model>


<!-- Nozzle -->

	<animation>
		<type>rotate</type>
		<object-name>NozzleShaft</object-name>
		<object-name>plume</object-name>
		<object-name>NozzlePlug</object-name>
		<object-name>Nozzle</object-name>
		<property>sim/multiplay/generic/float[13]</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>95</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
    			</entry>
		</interpolation>
		<center>
			<x-m>4.750</x-m>
			<y-m>0</y-m>
			<z-m>0.390</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>NozzleShaft</object-name>
		<object-name>FlameModel</object-name>
		<object-name>plume</object-name>
		<object-name>NozzlePlug</object-name>
		<object-name>Nozzle</object-name>
		<property>/surface-positions/nozzle-yaw</property>
		<interpolation>
			<entry>
				<ind>-1.0</ind>
				<dep>13</dep>
			</entry>
			<entry>
				<ind>0.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>-13</dep>
    			</entry>
		</interpolation>
		<center>
			<x-m>4.750</x-m>
			<y-m>0</y-m>
			<z-m>0.390</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
</PropertyList>