<!-- Transparent Surfaces for Rembrandt rendering -->
 <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>FixedCanopyGlass</object-name>
    <object-name>Canopy_1</object-name>
 </effect>

   <!-- Normal shader effect. Separate effects required for each normal map texture -->

 <effect>
    <inherits-from>Aircraft/F-35/Models/Effects/bumpspec-wing</inherits-from>
    <object-name>Wing</object-name>
    <object-name>RightAileron</object-name>
    <object-name>RightFlap</object-name>
    <object-name>RightSpoiler</object-name>
    <object-name>RightSlat</object-name>
    <object-name>LeftAileron</object-name>
    <object-name>LeftFlap</object-name>
    <object-name>LeftSpoiler</object-name>
    <object-name>LeftSlat</object-name>
 </effect>

 <effect>
    <inherits-from>Aircraft/F-35/Models/Effects/bumpspec-fuselage</inherits-from>
    <object-name>Fuselage</object-name>
 </effect>

 <effect>
    <inherits-from>Aircraft/F-35/Models/Effects/bumpspec-tail</inherits-from>
    <object-name>HStab</object-name>
    <object-name>RightElevator</object-name>
    <object-name>LeftElevator</object-name>
 </effect>
  	    
  
 <!-- Hide the gear when stowed -->
  <animation>
    <type>select</type>
    <object-name>NoseWheel</object-name>
    <object-name>NoseGearStrut</object-name>
    <object-name>NoseGearPiston</object-name>
    <object-name>NoseGearCylinder</object-name>
    <condition>
      <not>
        <equals>
          <property>gear/gear/position-norm</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>PortWheel</object-name>
    <object-name>PortGearStrut</object-name>
    <object-name>PortGearPiston</object-name>
    <condition>
      <not>
        <equals>
          <property>gear/gear[1]/position-norm</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>StarboardWheel</object-name>
    <object-name>RightGearStrut</object-name>
    <object-name>StarboardGearPiston</object-name>
    <condition>
      <not>
        <equals>
          <property>gear/gear[2]/position-norm</property>
          <value>0</value>
        </equals>
      </not>
    </condition>
  </animation>

