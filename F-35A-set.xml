	   <menu n="10">
	    <label>F-35A</label>
            <enabled type="bool">true</enabled>
	      <item>
		<label>Select Livery</label>
                <binding>
                <command>nasal</command>
                <script>aircraft.livery.dialog.toggle()</script>
                </binding>
	      </item>

           <item>
                <name>autostart</name>
                <label>Autostart | Autoshutdown</label>
                <binding>
                    <command>nasal</command>
                    <script>emu.autostart()</script>
                </binding>
            </item>
      <item>
			<label>Toggle Damage</label>
			<binding>
				<command>nasal</command>
				<script>
					if(getprop("/gear/gear/wow")){
						if(getprop("/payload/armament/msg")){screen.log.write("Multiplayer Damage Off",1,0,0);setprop("/payload/armament/msg",0);}else{screen.log.write("Multiplayer Damage On",1,0,0);setprop("/payload/armament/msg",1);}
					}else{
						screen.log.write("Damage can be toggled only when on the ground!",1,0,0);
					}
				</script>
			</binding>
		</item>
